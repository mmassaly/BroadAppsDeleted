{
	"routes" :
	[ 
		{
			"src": "/(.*)",
			"dest":"api/index.js"
		}
	]
	,
	"functions": {
		"api/*.js": {
		  "memory": 2056,
		  "maxDuration": 300
		}
	}
}

{
	
	"builds":
	[
		{
			"src" : "api/index.js",
			"use": "@vercel/node"
		}
	],
	"routes" :
	[ 
		{
			"src": "/(.*)",
			"dest":"api/index.js"
		}
	]
	
	
}